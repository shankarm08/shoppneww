<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="e.poojasharma.shopping">

    <uses-permission android:name="android.permission." />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/img2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Catalog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Productdetails" />
        <activity android:name=".ShoppingCart" />
        <activity android:name=".Checkout" />
        <activity android:name=".PaymentStatusActivity" />
        <activity android:name=".Quantity"></activity>
    </application>

</manifest>